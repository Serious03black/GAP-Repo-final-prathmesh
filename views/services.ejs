 <style>
    /* Golden gradient effect */
    .golden-gradient {
      background: linear-gradient(135deg, #caa437 0%, #ffd700 50%, #caa437 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .golden-border {
      border: 2px solid #caa437;
    }

    .golden-bg {
      background: #caa437;
      color: #000 !important;
    }

    .golden-bg:hover {
      background: #ffd700;
      transform: translateY(-3px);
      box-shadow: 0 10px 20px rgba(202, 164, 55, 0.3);
    }

    /* Zigzag service layout */
    .service-zigzag {
      position: relative;
      padding: 100px 0;
      background: #000;
      overflow: hidden;
    }

    .service-zigzag::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: radial-gradient(circle at 20% 50%, rgba(202, 164, 55, 0.1) 0%, transparent 50%),
        radial-gradient(circle at 80% 20%, rgba(202, 164, 55, 0.05) 0%, transparent 50%);
      z-index: 1;
    }

    .service-item-zigzag {
      background: rgba(20, 20, 20, 0.9);
      border-radius: 20px;
      padding: 40px;
      margin-bottom: 100px;
      position: relative;
      z-index: 2;
      border: 1px solid rgba(202, 164, 55, 0.2);
      transition: all 0.4s ease;
      backdrop-filter: blur(10px);
    }

    .service-item-zigzag:hover {
      transform: translateY(-10px);
      border-color: #caa437;
      box-shadow: 0 20px 40px rgba(202, 164, 55, 0.15);
    }

    .service-item-zigzag:nth-child(even) {
      margin-left: 50px;
    }

    .service-item-zigzag:nth-child(odd) {
      margin-right: 50px;
    }

    .service-content {
      flex: 1;
    }

    .service-image {
      flex: 1;
      border-radius: 15px;
      overflow: hidden;
      position: relative;
    }

    .service-image img {
      width: 100%;
      height: 400px;
      object-fit: cover;
      transition: transform 0.5s ease;
    }

    .service-item-zigzag:hover .service-image img {
      transform: scale(1.05);
    }

    .service-image::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(45deg, rgba(202, 164, 55, 0.1), transparent);
    }

    .service-icon {
      width: 70px;
      height: 70px;
      background: rgba(202, 164, 55, 0.1);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      margin-bottom: 25px;
      border: 2px solid #caa437;
    }

    .service-icon i {
      font-size: 32px;
      color: #caa437;
    }

    .service-title {
      font-size: 28px;
      margin-bottom: 20px;
      color: #caa437;
      font-weight: 600;
    }

    .service-features-zigzag {
      list-style: none;
      padding: 0;
      margin: 0 0 30px 0;
    }

    .service-features-zigzag li {
      color: #fff;
      margin-bottom: 12px;
      position: relative;
      padding-left: 25px;
      font-size: 16px;
      line-height: 1.6;
    }

    .service-features-zigzag li::before {
      content: '✦';
      position: absolute;
      left: 0;
      color: #caa437;
      font-size: 14px;
    }

    .service-cta-btn {
      background: transparent;
      color: #caa437;
      border: 2px solid #caa437;
      padding: 12px 30px;
      border-radius: 30px;
      font-weight: 600;
      text-transform: uppercase;
      letter-spacing: 1px;
      transition: all 0.3s ease;
      text-decoration: none;
      display: inline-block;
      font-size: 14px;
    }

    .service-cta-btn:hover {
      background: #caa437;
      color: #000;
      transform: translateX(5px);
    }

    /* Main CTA Section */
    .main-cta-section {
      background: linear-gradient(135deg, #000 0%, #111 100%);
      padding: 80px 0;
      text-align: center;
      position: relative;
      margin-top: 50px;
    }

    .main-cta-section::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1000 100"><path d="M0,50 Q250,0 500,50 T1000,50" fill="none" stroke="%23caa437" stroke-width="0.5" opacity="0.1"/></svg>');
      background-size: cover;
    }

    .main-cta-title {
      font-size: 42px;
      color: #fff;
      margin-bottom: 20px;
      font-weight: 600;
    }

    .main-cta-subtitle {
      font-size: 18px;
      color: #aaa;
      margin-bottom: 40px;
      max-width: 600px;
      margin-left: auto;
      margin-right: auto;
      line-height: 1.6;
    }

    .main-cta-button {
      background: linear-gradient(135deg, #caa437 0%, #ffd700 100%);
      color: #000;
      padding: 18px 45px;
      border-radius: 35px;
      font-size: 18px;
      font-weight: 600;
      text-transform: uppercase;
      letter-spacing: 1px;
      text-decoration: none;
      display: inline-block;
      transition: all 0.3s ease;
      box-shadow: 0 10px 30px rgba(202, 164, 55, 0.3);
    }

    .main-cta-button:hover {
      transform: translateY(-5px);
      box-shadow: 0 15px 40px rgba(202, 164, 55, 0.4);
      color: #000;
    }

    /* Responsive */
    @media (max-width: 992px) {
      .service-item-zigzag:nth-child(even),
      .service-item-zigzag:nth-child(odd) {
        margin-left: 0;
        margin-right: 0;
      }

      .service-image img {
        height: 300px;
      }

      .main-cta-title {
        font-size: 32px;
      }
    }

    @media (max-width: 768px) {
      .service-item-zigzag {
        padding: 30px;
        margin-bottom: 60px;
      }

      .service-title {
        font-size: 24px;
      }

      .main-cta-section {
        padding: 60px 20px;
      }
    }

    /* Section header */
    .section-header {
      text-align: center;
      padding: 80px 0 50px;
      background: #000;
    }

    .section-title-main {
      font-size: 48px;
      color: #caa437;
      margin-bottom: 20px;
      font-weight: 700;
      position: relative;
      display: inline-block;
    }

    .section-title-main::after {
      content: '';
      position: absolute;
      bottom: -10px;
      left: 50%;
      transform: translateX(-50%);
      width: 100px;
      height: 3px;
      background: linear-gradient(90deg, transparent, #caa437, transparent);
    }

    .section-subtitle {
      font-size: 18px;
      color: #fff;
      max-width: 700px;
      margin: 0 auto 30px;
      line-height: 1.6;
    }
  </style>
