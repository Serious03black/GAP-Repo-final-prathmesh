  <div class="input-field">
    <input
      type="tel"
      name="mobile"
      id="subscribeMobile"
      placeholder="Enter your mobile number"
      maxlength="10"
      inputmode="numeric"
      required
    >

    <button type="submit" class="subscribe-btn">
      <img src="/assets/imgs/icon/arrow-light.webp" alt="icon">
    </button>
  </div>

  <p id="subscribeError" style="color:red;font-size:13px;display:none;"></p>
  <p id="subscribeSuccess" style="color:green;font-size:13px;display:none;"></p>
</form>
<script>
  const form = document.getElementById("subscribeForm");
  const mobileInput = document.getElementById("subscribeMobile");
  const errorEl = document.getElementById("subscribeError");
  const successEl = document.getElementById("subscribeSuccess");

  form.addEventListener("submit", async function (e) {
    e.preventDefault(); // stop default form submit

    const mobile = mobileInput.value.trim();

    errorEl.style.display = "none";
    successEl.style.display = "none";

    // ✅ SIMPLE validation: number + 10 digits only
    if (!/^[6-9]\d{9}$/.test(mobile)) {
      errorEl.innerText = "Enter valid 10 digit mobile number";
      errorEl.style.display = "block";
      return;
    }

    try {
      const res = await fetch(form.action, {
        method: "POST",
        headers: {
          "Content-Type": "application/json"
        },
        body: JSON.stringify({ mobile })
      });

      const data = await res.json();

      if (!res.ok) {
        throw new Error(data.error || "Submission failed");
      }

      // ✅ success from backend
      successEl.innerText = data.message || "Submitted successfully";
      successEl.style.display = "block";
      mobileInput.value = "";

    } catch (err) {
      errorEl.innerText = err.message || "Server error";
      errorEl.style.display = "block";
    }
  });
</script>
