  <script>
    function openDemoPopup() {
      document.getElementById("demoPopupOverlay").style.display = "flex";
    }

    function closeDemoPopup() {
      document.getElementById("demoPopupOverlay").style.display = "none";
      document.getElementById("demoError").style.display = "none";
      document.getElementById("demoSuccess").style.display = "none";
      document.getElementById("demoMobile").value = "";
    }

    async function submitDemo() {
      const mobile = document.getElementById("demoMobile").value.trim();
      const error = document.getElementById("demoError");
      const success = document.getElementById("demoSuccess");
      const btn = document.getElementById("demoSubmitBtn");

      error.style.display = "none";
      success.style.display = "none";

      if (!/^[6-9]\d{9}$/.test(mobile)) {
        error.innerText = "Please enter a valid 10-digit mobile number";
        error.style.display = "block";
        return;
      }

      btn.innerText = "Submitting...";
      btn.disabled = true;

      try {
        const res = await fetch("/book-demo", {
          method: "POST",
          headers: { "Content-Type": "application/json" },
          body: JSON.stringify({ mobile })
        });

        const data = await res.json();

        if (!res.ok) throw new Error(data.error || "Something went wrong");

        if (data.success) {
          success.innerText = data.message;
          success.style.display = "block";
          setTimeout(() => closeDemoPopup(), 2000);
        } else {
          error.innerText = data.message || "Already registered";
          error.style.display = "block";
        }
      } catch (err) {
        error.innerText = err.message || "Server error. Try again.";
        error.style.display = "block";
      } finally {
        btn.innerText = "Submit";
        btn.disabled = false;
      }
    }
  </script>

